[user]
	name = kaoru.ohtaka
	email = ockacru+git@gmail.com
	signingkey = D9CD51C67F0EF53E
[commit]
	gpgsign = true
[merge]
	ff = false
	tool = vimdiff
	conflictstyle = diff3
[pull]
	ff = only
[core]
	ignorecase = false
	quotepath = false
	safecrlf = false
	autocrlf = false
	excludesFile = \"$HOME\"/.gitignore_global
[pager]
	branch = cat
[log]
	date = iso-local
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[diff]
	tool = vimdiff
[difftool]
	prompt = false
[mergetool]
	prompt = false
